<table class="table table-striped table-bordered mt-4">
  <thead class="table-dark">
  <tr>
    <th>ID</th>
    <th>Symbol</th>
    <th>Group</th>
  </tr>
  </thead>
  <tbody>
  <% @tickers.each do |ticker| %>
    <tr>
      <td><%= ticker.id %></td>
      <td><%= ticker.symbol %></td>
      <td><%= ticker.group.name %></td>
    </tr>
  <% end %>
  </tbody>
</table>
